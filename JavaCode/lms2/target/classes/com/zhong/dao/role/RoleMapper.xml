<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhong.dao.role.RoleMapper">
    <select id="queryRole" parameterType="_int" resultType="role">
        select * from lms2.role where id = #{id};
    </select>

    <select id="roleList" resultType="role">
        select * from lms2.role;
    </select>
</mapper>